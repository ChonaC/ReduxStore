{
  "name": "redux_store",
  "version": "1.0.0",
  "description": "Allows user to browse website and purchase items",
  "main": "Server/server.js",
  "scripts": {
    "start": "cd Server && npm start",
    "start:dev": "concurrently \"cd Server && npm run watch\" \"cd client && npm start\"",
