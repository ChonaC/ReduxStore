    "if-env": "^1.0.4",
    "nodemon": "^2.0.15"
  },
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "bugs": {
    "url": "https://github.com/ChonaC/Redux_Store/issues"
  },
  "homepage": "https://github.com/ChonaC/Redux_Store#readme"
